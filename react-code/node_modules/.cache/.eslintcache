     <Navbar></Navbar>\n      <Switch>\n        <Route path=\"/\" exact component={Home}></Route>\n      </Switch>\n    </Router>\n      {/* <nav className=\"nav\">\n        <h1>React test page</h1>\n      </nav> */}\n      \n      {/* <RouteTest></RouteTest>\n      <input type=\"button\" value=\"button\"/> */}\n      {/* <Contents/> */}\n      {/* 컴포넌트는 대분자로 시작해야한다. */}\n      {/* <Modal/> */}\n      {/* <Modal></Modal> */}\n    </>\n  );\n}\n\n\nexport default App;","/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Navbar.js",["59","60","61","62","63"],"import React, {useState, useEffect} from 'react'\nimport './Navbar.css';\nimport Button from './Button'\nimport {\n    BrowserRouter as Router,\n    Switch,\n    Route,\n    Link,\n    NavLink,\n    useParams\n  } from \"react-router-dom\";\n\n\nfunction Navbar() {\n    const [click, setClick] = useState(false);\n    const [button, setButton] = useState(true);\n\n    // const handleClick = () => setClick(!click);\n    const handleClick = function(){\n        setClick(!click);\n    }\n\n    const closeMobileMenu = () => setClick(false);\n\n    const showButton = () => {\n        if(window.innerWidth <= 960){\n            setButton(false);\n        } else {\n            setButton(true);\n        }\n    };\n\n    useEffect(() => {\n        showButton();\n    },);\n\n    window.addEventListener('resize', showButton);\n    \n\n    return(\n        <>\n            <nav className=\"navbar\">\n                <div className=\"navbar-contatiner\">\n                    <Link to=\"/\" className=\"navbar-logo\" onClick={closeMobileMenu}>\n                        DKsladkfjlask\n                    </Link>\n                    {/* 클릭시 클래스명을 변경하여 CSS속성을 변경하는 토글 스위치 */}\n                    <div className=\"menu-icon\" onClick={handleClick}>\n                        <p className={click ? 'openmenu' : 'closedmenu'}>menu</p>\n                    </div>\n                    <ul className={click ? 'nav-menu active' : 'nav-menu'}>\n                        <li className=\"nav-item\">\n                            <Link to=\"/\" className=\"nav-links\" onClick={closeMobileMenu}>\n                                Home\n                            </Link>\n                        </li>\n                        <li className=\"nav-item\">\n                            <Link to=\"/services\" className=\"nav-links\" onClick={closeMobileMenu}>\n                                Services\n                            </Link>\n                        </li>\n                        <li className=\"nav-item\">\n                            <Link to=\"/products\" className=\"nav-links\" onClick={closeMobileMenu}>\n                                Products\n                            </Link>\n                        </li>\n                        <li className=\"nav-item\">\n                            <Link to=\"/sign-up\" className=\"nav-links-mobile\" onClick={closeMobileMenu}>\n                                Sign up\n                            </Link>\n                        </li>\n                    </ul>\n                    {button && <Button buttonStyle=\"btn--outline\">Sign up</Button>}\n                </div>        \n            </nav>\n        </>\n    )\n}\n\nexport default Navbar;","/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Modal.js",["64","65"],"import React, {useState} from 'react'\n\nfunction Modal(){\n  var [title, setTitle] = useState();\n    return(\n      <div className=\"modal\">\n          <h2>제목</h2>\n          <p>날짜</p>\n          <p>로렘입숨이ㅏㄹ머니아러가나다라마바사아자차카타파하</p>\n        </div>\n    );\n  }\n\n  export default Modal;","/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/RouteTest.js",["66","67","68"],"import React, {useState} from 'react';\nimport {\n    BrowserRouter as Router,\n    Switch,\n    Route,\n    Link,\n    NavLink,\n    useParams\n  } from \"react-router-dom\";\n\n  var contents = [\n    {id:1, title: \"HTML\", desc:\"HTML is...\"},\n    {id:2, title: \"JS\", desc:\"JS is...\"},\n    {id:3, title: \"REACT\", desc:\"REACT is...\"}\n  ]\n  \n  function Topic(){\n    var params = useParams();\n    var topic_id = params.topic_id;\n    var seleted_topic = {\n      title:\"Sorry\",\n      desc:\"Not Found\"\n    }\n    for(var i=0; i<contents.length; i++){\n      if(contents[i].id === Number(topic_id)){\n        seleted_topic = contents[i];\n        break;\n      }\n    }\n  \n    console.log(params);\n  \n    return(\n      <>\n        <h3>{seleted_topic.title}</h3>\n        <p>{seleted_topic.desc}....</p>\n      </>\n    );\n  }\n  \n  function Topics(){\n    var list = [];\n    for(var i=0; i<contents.length; i++){\n      list.push(\n      <li key={contents[i].id}>\n        <NavLink to={'/topics/'+contents[i].id}>\n          {contents[i].title}\n        </NavLink>\n      </li>)\n    }\n  \n    return(\n      <>\n      <h2>Topics</h2>\n      <ul>\n        {list}\n      </ul>\n      <Route path=\"/topics/:topic_id\">\n        <Topic></Topic>\n      </Route>\n      {/* <Switch>\n        <Route path=\"/topics/1\">HTML is...</Route>\n        <Route path=\"/topics/2\">JS is...</Route>\n        <Route path=\"/topics/3\">REACT is...</Route>\n      </Switch> */}\n      </>\n    );\n  }\n\n  function Contact(){\n    return(\n      <>\n      <h2>Contact</h2>\n      <p>Contact..</p>\n      </>\n    );\n  }\n\n  function Home(){\n    return(\n      <>\n      <h2>Home</h2>\n      <p>Home..</p>\n      </>\n    );\n  }\n\n  function RouteTest(){\n\n    return(\n      <>\n      {/* NavLink는 선택된 ui에 active라는 클래스를 추가하도록 되어있음.\n      따라서, css를 변경해서 navigation 이 가능해짐. */}\n      <ul>\n        <li><NavLink exact to=\"/\">Home</NavLink></li>\n        <li><NavLink exact to=\"/topics\">Topics</NavLink></li>\n        <li><NavLink exact to=\"/contact\">Contact</NavLink></li>\n      </ul>\n      {/* 라우터와 일치하는 path를 가진 컴포넌트는 화면에 출력된다. \n      exact path를 사용하면 정확히 동일한 경우에만