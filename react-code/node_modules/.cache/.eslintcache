eErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"76"},"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/index.js",[],["155","156"],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/App.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Navbar.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/RouteTest.js",["157","158","159"],"import React, {useState} from 'react';\nimport {\n    BrowserRouter as Router,\n    Switch,\n    Route,\n    Link,\n    NavLink,\n    useParams\n  } from \"react-router-dom\";\n\n  var contents = [\n    {id:1, title: \"HTML\", desc:\"HTML is...\"},\n    {id:2, title: \"JS\", desc:\"JS is...\"},\n    {id:3, title: \"REACT\", desc:\"REACT is...\"}\n  ]\n  \n  function Topic(){\n    var params = useParams();\n    var topic_id = params.topic_id;\n    var seleted_topic = {\n      title:\"Sorry\",\n      desc:\"Not Found\"\n    }\n    for(var i=0; i<contents.length; i++){\n      if(contents[i].id === Number(topic_id)){\n        seleted_topic = contents[i];\n        break;\n      }\n    }\n  \n    console.log(params);\n  \n    return(\n      <>\n        <h3>{seleted_topic.title}</h3>\n        <p>{seleted_topic.desc}....</p>\n      </>\n    );\n  }\n  \n  function Topics(){\n    var list = [];\n    for(var i=0; i<contents.length; i++){\n      list.push(\n      <li key={contents[i].id}>\n        <NavLink to={'/topics/'+contents[i].id}>\n          {contents[i].title}\n        </NavLink>\n      </li>)\n    }\n  \n    return(\n      <>\n      <h2>Topics</h2>\n      <ul>\n        {list}\n      </ul>\n      <Route path=\"/topics/:topic_id\">\n        <Topic></Topic>\n      </Route>\n      {/* <Switch>\n        <Route path=\"/topics/1\">HTML is...</Route>\n        <Route path=\"/topics/2\">JS is...</Route>\n        <Route path=\"/topics/3\">REACT is...</Route>\n      </Switch> */}\n      </>\n    );\n  }\n\n  function Contact(){\n    return(\n      <>\n      <h2>Contact</h2>\n      <p>Contact..</p>\n      </>\n    );\n  }\n\n  function Home(){\n    return(\n      <>\n      <h2>Home</h2>\n      <p>Home..</p>\n      </>\n    );\n  }\n\n  function RouteTest(){\n\n    return(\n      <>\n      {/* NavLink는 선택된 ui에 active라는 클래스를 추가하도록 되어있음.\n      따라서, css를 변경해서 navigation 이 가능해짐. */}\n      <ul>\n        <li><NavLink exact to=\"/\">Home</NavLink></li>\n        <li><NavLink exact to=\"/topics\">Topics</NavLink></li>\n        <li><NavLink exact to=\"/contact\">Contact</NavLink></li>\n      </ul>\n      {/* 라우터와 일치하는 path를 가진 컴포넌트는 화면에 출력된다. \n      exact path를 사용하면 정확히 동일한 경우에만 작동한다 */}\n      {/* 스위치로 컴포넌트를 감싸면, 가장 먼저 감지되는 컨텐츠만 화면에 출력한다. \n      편법으로, 홈을 제일 뒤에 주면 가능 동일하지 않은 것들을 not found로 출력가능*/}\n      <Switch>\n        <Route exact path=\"/\"><Home></Home></Route>\n        <Route path=\"/topics\"><Topics></Topics></Route>\n        <Route path=\"/contact\"><Contact></Contact></Route>\n        <Route path=\"/\">Not Found</Route>\n      </Switch>\n      </>\n    );\n  }\n\n  export default RouteTest;",["160","161"],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Contents.js",["162","163","164","165","166","167"],"import React, {useState} from 'react';\nimport { BrowserRouter as Router, Switch, Route, Link, NavLink, useParams } from \"react-router-dom\";\n\nvar contentsData = [\n    {id: 1, title:\"First title\", date:\"2021.09.\", desc:\"First blavla\"},\n    {id: 2, title:\"Second title\", date:\"2021.09.\", desc:\"Second blavla\"},\n    {id: 3, title:\"Third title\", date:\"2021.09.\", desc:\"Third blavla\"}\n];\n\nfunction Contents(props) {\n\n    var lists = [];\n    var i = 0;\n\n    for (i=0; i < contentsData.length; i++){\n      lists.push(\n        <div className = \"list\">\n          <Link to={\"/contents/\"+contentsData[i].id}><h3>{contentsData[i].title}</h3></Link>\n          <p>{contentsData[i].date}{10+i}</p>\n          <hr/>\n        </div>\n      )\n    }\n\n    return(\n      <>\n      {lists}\n      </>\n    );\n  }\n\n  export default Contents;","/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Button.js",["168","169","170","171","172","173","174","175"],"import React from 'react';\nimport './Button.css';\nimport {\n    BrowserRouter as Router,\n    Switch,\n    Route,\n    Link,\n    NavLink,\n    useParams\n  } from \"react-router-dom\";\n\n  const STYLES = ['btn--primary', 'btn--outline'];\n  const SIZES = ['btn--meidum', 'btn--large'];\n\nexport const Button = ({\n    children, \n    type, \n    onClick,\n    buttonStyle, \n    buttonSize}) => {\n        const checkButtonStyle = STYLES.includes(buttonStyle)\n            ? buttonStyle\n            : STYLES[0]\n\n            const checkButtonSize = SIZES.includes(buttonSize)\n            ? buttonSize\n            : SIZES[0]\n\n            return (\n                <Link to=\"/sign-up\" className=\"btn-mobile\">\n                    <button className={'btn ${checkButtonStyle} ${checkButtonSize}'}\n                    onClick={onClick}\n                    type={type}\n                    >\n                        {children}\n                    </button>\n                </Link>\n            )\n    }\n\nexport default Button\n",["176","177"],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Home.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Cards.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Carditem.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/MainSection.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Introduce.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work01.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/WorkIntro.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/Footer.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/ScrollToTop.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/About.js",["178"],"import React, {useState} from 'react';\nimport '../About.css';\n\nfunction About() {\n    return (\n       <>\n       <div className=\"about-greeting\">\n           <div className=\"about-greeting__text --en\">\n                <p>Hello.</p>\n                <p><span className=\"outline-text\">Designer</span>ㅤ―ㅤ<span className=\"outline-text\">Developer</span></p>\n                \n                <p>I want to close the gap.</p>\n                <p>I'm Daekyo Jeong.</p>\n            </div>\n            <div className=\"about-greeting__text --ko\">\n                <p className=\"about-greeting__p--first\">안녕하세요.</p>\n                <div className=\"about-greeting__dd--virtical--ko\">\n                    <p className=\"outline-text\">디자이너</p>\n                    <p className=\"outline-text\">디벨로퍼</p>\n                </div>\n                <div className=\"about-greeting__dd--ko\" >\n                    <p className=\"outline-text\">디자이너</p>\n                    <p className=\"outline-text\">디벨로퍼</p>\n                </div>\n                <p className=\"about-greeting__p--second\">간극을 좁혀보고픈</p>\n                <p className=\"about-greeting__p--third\">정대교입니다.</p>\n            </div>\n        </div>\n        <div className=\"about-introduce\">\n            <div className=\"about-introduce__text --ko\">\n                <h1>저는 이런 사람입니다.</h1>\n                <br />\n                <p>프론트엔드 개발과 디자인, 사용자 경험・인터페이스 디자인에 <br/>관심이 있고 다양한 인터랙션 디자인을 공부하고 있습니다.</p>\n                <br />\n                <p><span className=\"bold-text\">새로운 것을 탐구하고 배우는 것에 상당한 흥미</span>를 느낍니다.</p>\n                <p><span className=\"bold-text\">문제해결에 집요한 시간</span>을 쏟습니다.</p>\n                <p>저의 호기심과 집요함은 디자인적 역량을 바탕으로 <br/>\n                <span className=\"bold-text\">좋은 인터랙션에 접근하기 위해 끈임없이 고민</span>합니다.</p>\n                <br />\n                <p>끈임없이 고민했던 노력의 흔적을 내 손으로 구현할 수 있다는 것이 <br/>디벨로퍼라는 역할은 매력적으로 다가왔습니다.</p>\n                <p>사용자의 입장에서 끈임없이 고민하는 <br /><span className=\"bold-text\">인터랙션 디자이너이자 풀스택 디벨로퍼</span>를 꿈꾸고 있습니다.</p>\n            </div>\n            <br />\n            <p>dk_w_rld@naver.com</p>\n        </div>\n        <div className=\"about-table\">\n            <Table \n            title=\"경력\">\n            <TableData \n            date=\"20.03 - 21.03\"\n            desc=\"홍익대학교 디자인컨버전스학부 실습조교\"/></Table>\n\n            <Table \n            title=\"학력\">\n            <TableData \n            date=\"14.03 - 20.02\"\n            desc=\"홍익대학교 디지털미디어디자인전공 졸업\"/></Table>\n\n            <Table \n            title=\"경험\">\n            <TableData \n            date=\"18.12 - 19.12\"\n            desc={\"디지털미디어디자인전공 졸업준비 위원회 \\n 위원장\"}/>\n            <TableData \n            date=\"18.03 - 18.12\"\n            desc={\"디지털미디어디자인전공 학생회 \\n 기획부원\"}/>\n            <TableData \n            date=\"18.03 - 18.12\"\n            desc={\"인스톨레이션 소모임 '비본' \\n 스튜디오 팀장\"}/>\n            <TableData \n            date=\"18.06 - 18.08\"\n            desc={\"홍익대학교 거리미술전 \\n 작품 '동전의 공중회전' 제작 및 설치\"}/></Table>\n\n            <Table \n            title=\"수상\">\n            <TableData \n            date=\"18.11\"\n            desc={\"조형대학 창립 30주년 과제전 \\n 3학년 대표 우수상\"}/>\n            <TableData \n            date=\"15.08\"\n            desc={\"세상을 바꾸는 인포그래픽 \\n 우수상\"}/></Table>\n        </div>\n       </>\n    )\n}\n\nfunction Table(props) {\n    return(\n        <>\n        <div className=\"table\" key={props.index}>\n            <h2 className=\"table-title\">{props.title}</h2>\n            <div className=\"table-grid\">{props.children}</div>\n        </div>\n        </>\n    )\n}\n\n//\\n 문자열을 기준으로 텍스트를 분할하여서 하단에 br태그를 추가해주는 방식\nfunction TableData(props){\n    return(\n        <>\n            <p className=\"table-date\">{props.date}</p>\n            <p className=\"table-desc\">\n                {props.desc.split(\"\\n\").map((txt) => (\n                <>\n                    {txt}\n                    <br />\n                </>\n                ))}\n            </p>\n        </>\n    )\n}\n\nexport default About;\n","/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/WorkImageBox.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/WorkTextBox.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/GridCardItem.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Lab.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work02.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/WorkImageBoxGrid.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Labs/Labs01.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/ImageSlider.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Labs/Labs02.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Labs/Labs03.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work03.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work04.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work05.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/components/pages/Works/Work06.js",[],"/Users/dk/Desktop/works/Project/coding/dk_world_react/react-code/src/com